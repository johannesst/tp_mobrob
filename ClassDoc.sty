\def\DataTabCheck#1{%
  \settowidth{\global\CD@DummyLen}{#1}%
  \divide \CD@DummyLen by \CD@ColDLen%
  \setcounter{CD@cntDummy}{\CD@DummyLen}%
  \ifnum \value{CD@cntDummy}<\value{MaxMoreCnt}%
  \setcounter{CD@cntDummy}{\value{MaxMoreCnt}}\fi%
  \addtocounter{CD@cntDummy}{3}%
  \ifnum \value{CD@cntDummy} >\value{CD@LinesLeft}%
    \ifnum \value{NewDataTable}>1%
      \setlength{\CD@PagePos}{\textheight}%
      \CD@DummyLen=\baselineskip%
      \divide\CD@PagePos by \CD@DummyLen%
      \setcounter{CD@LinesLeft}{\CD@PagePos}%
      \addtocounter{CD@LinesLeft}{-4}%
      \setcounter{CD@PageCorrector}{1}%
    \else\setcounter{CD@cntDummy}{\value{page}}%
      \addtocounter{CD@cntDummy}{\value{CD@PageCorrector}}%
      \stepcounter{CD@cntDummy}\\ \hline%
      \multicolumn{\value{CD@NoOfCols}}{r}{\footnotesize Fortsetzung auf Seite \arabic{CD@cntDummy}}\\%
      \end{tabular}\setcounter{CD@LastPage}{\value{page}}%
      \addtocounter{CD@LastPage}{\value{CD@PageCorrector}}\setcounter{NewDataTable}{1}%
      \setcounter{CD@PageCorrector}{0}%
      \newpage\CD@CalcLinesLeft%
      \ifnum \value{CD@NoOfCols}=3%
        \begin{tabular}{|p{\CD@ColBLen}|p{\CD@ColCLen}|l|}%
        \multicolumn{3}{l}{\footnotesize Fortsetzung von Seite \arabic{CD@LastPage}}\\ \hline%
      \else%
        \begin{tabular}{|p{\CD@ColALen}|p{\CD@ColBLen}|p{\CD@ColCLen}|l|}%
        \multicolumn{3}{l}{\footnotesize Fortsetzung von Seite \arabic{CD@LastPage}}\\ \hline%
      \fi%
      \addtocounter{CD@LinesLeft}{-2}\Headline%
    \fi\fi%
  \ifnum\value{NewDataTable}>1%
    \ifnum\value{CD@NoOfCols}=3%
      \begin{tabular}{|p{\CD@ColBLen}|p{\CD@ColCLen}|l|}\hline%
    \else%
      \begin{tabular}{|p{\CD@ColALen}|p{\CD@ColBLen}|p{\CD@ColCLen}|l|}\hline%
