%% Kommandos zur Erzeugung der Schluesselwoerter:
\def\PKey{\bf}
\def\TitleA{{\PKey{procedure }}}\def\TitleB{}
\def\WhileA{{\PKey{while (}}}\def\WhileB{{\PKey{) do}}}
  \def\WhileC{{\PKey{od}}}
\def\ForA{{\PKey{for (}}}\def\ForB{{\PKey{) do}}}
  \def\ForC{{\PKey{od}}}
\def\RepeatA{{\PKey{repeat}}}\def\RepeatB{{\PKey{until (}}}
  \def\RepeatC{{\PKey{)}}}
\def\IfA{{\PKey{if (}}}\def\IfB{{\PKey{) then}}}
  \def\IfC{{\PKey{fi}}}
\def\ElsifA{{\PKey{elsif (}}}\def\ElsifB{{\PKey{) then}}}
\def\ElseA{{\PKey{else}}}
\def\SwitchAA{{\PKey{switch \{}}}
\def\SwitchBA{{\PKey{switch (}}}\def\SwitchBB{{\PKey{) \{}}}
\def\SwitchC{{\PKey{\}}}}
\def\CaseA{{\PKey{case (}}}\def\CaseB{{\PKey{)}}}
\def\DefaultA{{\PKey{default}}}
\def\CallA{{\PKey{call }}}\def\CallB{}

%%----------------------
%% Pseudo-Code Umgebung
%%----------------------
\def\PseudoCode{\CD@ifnextchar [{\PSEUDO}{\PSEUDO[]}}
\def\PSEUDO[#1]{
  \global\newcount\c@MARKno
  \c@MARKno=0
  \ifx\CD@empty#1\CD@empty\else {\par\TitleA#1\TitleB\vspace{\parskip}}\fi
  \parskip0pt

  \def\SETmark{}
  \def\newline{\SETmark\par\def\SETmark{}}

%% Pseudo-Code Befehle
%% -------------------
  \def\While##1{\newline\WhileA##1\WhileB\newline\DEEPER}
  \def\EndWhile{\end{list}\WhileC\newline}

  \def\For##1{\newline\ForA##1\ForB\newline\DEEPER}
  \def\EndFor{\end{list}\ForC\newline}

  \def\Repeat{\newline\RepeatA\DEEPER}
  \def\Until##1{\newline\end{list}\RepeatB##1\RepeatC\newline}

  \def\If##1{\newline\IfA##1\IfB\newline\DEEPER}
  \def\Elsif##1{\newline\end{list}\ElsifA##1\ElsifB
    \newline\DEEPER}
  \def\Else{\newline\end{list}\ElseA\newline\DEEPER}
  \def\EndIf{\newline\end{list}\IfC\newline}

  \def\Switch{\def\INswitch{\def\INswitch{\end{list}}}
    \CD@ifnextchar [{\SWITCHby}{\DIRECTswitch}}
  \def\DIRECTswitch{\newline\SwitchAA\DEEPER}
  \def\SWITCHby[##1]{\newline\SwitchBA##1\SwitchBB\newline\DEEPER}
  \def\Case##1{\newline\INswitch\CaseA##1\CaseB\newline\DEEPER}
  \def\Default{\newline\INswitch\DefaultA\newline\DEEPER}
  \def\EndSwitch{\newline\INswitch\end{list}\SwitchC\newline}

  \def\Call##1{\newline\CallA##1\CallB\newline}

%% Befehl fuer Art der Markierung
  \def\theMARKno{(\arabic{MARKno})}

%% Befehl zur Markierung von Zeilen im Pseudocode (mit oder ohne Label)
  \def\Mark{\CD@ifnextchar [{\REFmark}{\REFmark[]}}
  \def\REFmark[##1]{\def\SETmark{\global\refstepcounter{MARKno}
    \edef\@currentlabel{\csname theMARKno\endcsname}%
    \ifx\CD@empty##1\CD@empty\else \label{##1}\fi
    \hfill{\bf\theMARKno}}}
}

\def\endPseudoCode{\newline}

%%---------------------------------------------------------------------------
%% interner Befehl fuer Einrueckungen mittels der List-Umgebung
%%---------------------------------------------------------------------------
  \def\DEEPER{\begin{list}{}{%
    \setlength\itemsep{0pt}\setlength\parsep{0pt}%
    \setlength\parskip{0pt}\setlength\topsep{0pt}%
    \setlength\listparindent{0em}\setlength\leftmargin{1.5em}%
    \setlength\itemindent{0em}\setlength\partopsep{0pt}}%
    \item[]}
%%---------------------------------------------------------------------------

%%---------------------------------------------------------------------------
%% Befehl zur Ermittlung der Anzahl der Elemente einer durch Komma getrennten
%% Liste von Eintraegen
%%---------------------------------------------------------------------------
\def\NoOfParams#1{\setcounter{CD@ParamCount}{0}%
  \CntParams#1,[]]\arabic{CD@ParamCount}}
\def\CntParams#1,#2[]]{\ifx \CD@empty#2\CD@empty
    \ifx \CD@empty#1\CD@empty
    \else \stepcounter{CD@ParamCount}\fi
  \else \stepcounter{CD@ParamCount}\CntParams#2[]]\fi }
%%---------------------------------------------------------------------------

%%---------------------------------------------------------------------------
%% Befehl zur Ausgabe der Kurzuebersicht ueber Klassen & Methoden
%%---------------------------------------------------------------------------
\def\ClassOverview{\CD@ifnextchar({\CD@Overview}{\CD@Overview()}}
\def\CD@Overview(#1){\CD@ifnextchar[{\CD@COV{#1}}{\CD@COV{#1}[]}}
\def\CD@COV#1[#2]{{\ifx \CD@empty#2\CD@empty%
    \immediate\closeout\ClassViewFile%
    \def\CD@COFname{\jobname.cof}%
  \else \def\CD@COFname{#2}\fi
  \openin1=\CD@COFname\relax
  \ifeof1 \closein1\typeout{no class-overview-file '\CD@COFname'}\else
    \closein1
    \ifx\CD@empty#1\CD@empty
      \def\CD@Headline{\ClassSection{"Ubersicht "uber die Klassen}}
      \def\rightmark{\theClassSection{} "Ubersicht "uber die Klassen}
      \def\leftmark{\theClassSection{} "Ubersicht "uber die Klassen}
    \else%
      \def\CD@Headline{\ClassSection{#1}}
      \def\rightmark{\theClassSection{} #1}
      \def\leftmark{\theClassSection{} #1}
    \fi%
    \newcounter{CD@ViewState}
    \setlength{\CD@DummyLen}{\textwidth}%
    \addtolength{\CD@DummyLen}{-1\columnsep}%
    \setlength{\CD@BoxWidth}{.5\CD@DummyLen}%
    \def\Class##1##2{\rule{\CD@BoxWidth}{.5pt}\vskip1.5ex%
      {\large\niCR{##1}}%
      \global\setcounter{CD@ViewState}{0}%
      \hangindent=5em\hangafter=1%
      \ifx\CD@empty##2\CD@empty\else \quad$\rightarrow$ ##2\fi\vskip.5ex\par}
    \def\Father##1{\hspace{2em}Basisklasse:
      \settowidth{\CD@DummyLen}{Basisklasse: }%
      \addtolength{\CD@DummyLen}{2em}\hangafter=1 \hangindent=\CD@DummyLen%
      \CR{##1}\par}
    \def\Public##1##2{\ifnum\value{CD@ViewState}=1 \else%
      \ifnum\value{CD@ViewState}>0\fi \hspace{2em}\"offentliche Methoden:
      \par\fi\hangindent=5em\hangafter=1%
      \global\setcounter{CD@ViewState}{1}\hspace{4em}\niMR{##1} (##2)\par}
    \def\Protected##1##2{\ifnum\value{CD@ViewState}=2 \else%
      \ifnum\value{CD@ViewState}>0 \fi \hspace{2em}gesch\"utzte Methoden:
      \par\fi\hangindent=5em\hangafter=1%
      \global\setcounter{CD@ViewState}{2}\hspace{4em}\niMR{##1} (##2)\par}
    \def\EndClass{\ifnum\value{CD@ViewState}>0\fi%
      \global\setcounter{CD@ViewState}{0}\par\vskip1.5ex%
      \rule[2pt]{\CD@BoxWidth}{.5pt}\hskip-\CD@BoxWidth}
    {\parskip0pt%
     \if@twocolumn[\CD@Headline]\input{\CD@COFname}
     \else\twocolumn[\CD@Headline]\input{\CD@COFname}\onecolumn\fi}
  \fi}}

\endinput
